{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "type": "object",
  "title": "File Info",
  "properties": {
    "name": {
      "type": "string",
      "minLength": 1
    },
    "size": {
      "type": "integer",
      "format": "uint64",
      "minimum": 0
    }
  },
  "required": [ "name", "size" ],
  "unevaluatedProperties": false
}