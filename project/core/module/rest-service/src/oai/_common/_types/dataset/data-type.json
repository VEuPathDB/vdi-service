{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "Dataset Type Details",
  "description": "Details about a specific dataset type.",
  "type": "object",
  "properties": {
    "name": {
      "type": "string",
      "minLength": 3
    },
    "version": {
      "type": "string",
      "minLength": 1
    },
    "displayName": {
      "type": "string",
      "minLength": 3
    }
  },
  "required": [
    "name",
    "version",
    "displayName"
  ],
  "unevaluatedProperties": false
}